# Domus Backend Configuration
# Copy this file to .env and fill in your values

# Gemini API Key (get from https://aistudio.google.com/app/apikey)
DOMUS_GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Change the model (default: gemini-1.5-flash)
# DOMUS_GEMINI_MODEL=gemini-1.5-pro

# Redis URL (optional - defaults to in-memory storage)
# DOMUS_REDIS_URL=redis://localhost:6379

# JWT Secret (change in production!)
# DOMUS_JWT_SECRET=your-super-secret-key

# Debug mode
# DOMUS_DEBUG=true
